        "### Load the DataFrame again, this time with the inferSchema option enabled\n",
        "housingdf = (spark\n",
        "            .read\n",
        "            .option('header',True)\n",
        "            .option('inferSchema',True)\n",
        "            .csv(usersCsvPath))\n",
        "housingdf.printSchema()"
      ]
